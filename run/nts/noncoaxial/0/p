FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 -1 0 0 0];

internalField   uniform 0;

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    outer
    {
        type            fixedFluxPressure;
        gradient        0;
    }
    inner
    {
        type            fixedFluxPressure;
        gradient        0;
    }
    symmetry
    {
        type            symmetryPlane;
    }
}

