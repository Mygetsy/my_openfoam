FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.metal 0
);

high 1e5;
low -1e5;

regions
(
    boxToCell
    {
        box ($low $low $low) ($high $high 0);
        fieldValues
        (
            volScalarFieldValue alpha.metal 1
        );
    }
);


// ************************************************************************* //
