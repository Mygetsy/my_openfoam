FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ K ];

internalField   uniform 293;

boundaryField
{
    bottom
    {
        type            fixedValue;
        value           $internalField;
    }
    top
    {
        type            fixedValue;
        value           $internalField;
    }
    side
    {
        type            fixedValue;
        value           $internalField;
    }
}

