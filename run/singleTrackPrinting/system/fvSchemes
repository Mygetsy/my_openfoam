FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    //default         Gauss linear;
    div(((rho*U)&devRhoReff)) Gauss linear;
    div(((rho*nuEff)*dev2(T(grad(U))))) Gauss linear;
    div(rhoPhi,U)   Gauss linearUpwind grad(U);
    div(rhoPhi,he)  Gauss linearUpwind grad(he); // limitedLinear -- Koren limiter
    //div(phi,alpha)  Gauss vanLeer;
}

laplacianSchemes
{
    default         Gauss linear orthogonal; // orthogonal is used for Cartesian meshes
    //laplacian(rAUf,pcorr) Gauss linear orthogonal;
    //laplacian((rho*nuEff),U) Gauss linear orthogonal;
    //laplacian(rAUf,p_rgh) Gauss linear orthogonal;
    //laplacian(((thermalConductivity|heatCapacity)*(1-((alpha.metal*dMeltIndicator)*enthalpyFusion))),he) Gauss limitedLinear diffusivityf 1 orthogonal;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         orthogonal;
}

wallDist
{
    method meshWave;
}

// ************************************************************************* //
