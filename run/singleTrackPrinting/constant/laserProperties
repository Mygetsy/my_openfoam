FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      laserProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

laserPower      laserPower [ W ] 113; // 113 for Trumpf TruPrint 1000
absorptivity    absorptivity [ ] 0.35;
laserRadius     laserRadius [ m ] 2.75e-5; // 2.75e-5 for Trumpf TruPrint 1000
laserVelocity   laserVelocity [ m/s ] ( 0.7 0 0 ); // 0.7 for Trumpf TruPrint 1000
coordStart      coordStart [ m ] ( 0 0 0 );
timeStop        timeStop [ s ] 0.0005;

