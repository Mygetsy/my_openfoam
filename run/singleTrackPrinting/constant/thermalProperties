FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      thermalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Sources:
// Air -- https://www.engineersedge.com/physics/viscosity_of_air_dynamic_and_kinematic_14483.htm
// Argon -- J. Kestin et al. Equilibrium and Transport Properties of the Noble Gases and Their Mixtures at Low Density.
//      Journal of Physical and Chemical Reference Data 13, 229 (1984).
// SS316L -- C. S. Kim. Thermophysical properties of stainless steels. Tech. rep. Argonne National Lab., Ill.(USA), 1975.
// SS316L -- K. C. Mills. Recommended values of thermophysical properties for selected commercial alloys. Woodhead Publishing, 2002.

k_gas           k_gas [ W/m/K ] 6.16e-2; // Argon: 6.16e-2 for liquidus
// Air: 2.5e-2 for 293K, 1.1e-1 for 2000K
Cp_gas          Cp_gas [ J/kg/K ] 520; // = 2.5*R/M;
// Air: 1200 for liquidus, 1007 for 293K, 1264 for 2000K

// From Kim
k_sol           k_sol [ W/m/K ] 9.248;
dk_sol          dk_sol [ W/m/K^2 ] 1.571e-2;
k_liq           k_liq [ W/m/K ] 12.41;
dk_liq          dk_liq [ W/m/K^2 ] 3.279e-3;

Cp_sol          Cp_sol [ J/kg/K ] 459.0;
dCp_sol         dCp_sol [ J/kg/K^2 ] 0.1328;
Cp_liq          Cp_liq [ J/kg/K ] 769.9;
dCp_liq         dCp_liq [ J/kg/K^2 ] 0;

T_ambient       T_ambient [ K ] 293;
T_melting       T_melting [ K ] 1700; // Kim: 1700, Mills: solidus=1633, liquidus=1683
T_boiling       T_boiling [ K ] 3090; // Kim

enthalpyFusion  enthalpyFusion [ J/kg ] 2.7e+5;
enthalpyBoiling enthalpyBoiling [ J/kg ] 7.41e+6;

sigmoid         erf; //tanh;

// ************************************************************************* //
