FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal air);

metal
{
    //transportModel  Newtonian;
    transportModel  trueArrheniusNewtonian;
    trueArrhenius<Newtonian>Coeffs
    {
        alpha       5484;
        Talpha      1673;
    };
    nu              9.796e-7; // dynamic viscosity = 6.759e-3;
    rho             6.9e3;
}

air
{
    transportModel  Newtonian;
    nu              1.5e-5; // 1.516e-5 for 293K, 4.270e-4 for 2000K
    rho             1; // 1.204 for 293 K, 0.1553 for 2000K
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial
    (
        (3.282      0)
        (-0.00089   1)
    );
}

permeability    permeability    [ 1 -3 -1 0 0 0 0 ] 1e+3; // 1e+12 for PDAS=750nm

// ************************************************************************* //
