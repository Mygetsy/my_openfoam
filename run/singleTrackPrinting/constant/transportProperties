FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal air);

metal
{
    transportModel  Newtonian;
    nu              8.21e-4; // 5.2e-3 for Ti-6Al-4V;
    rho             6.9e3;
}

air
{
    transportModel  Newtonian;
    nu              1.48e-05;
    rho             1;
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial (
        (3.282      0)
        (-0.00089   1)
    );
}

permeability    permeability    [ 1 -3 -1 0 0 0 0 ] 5e+2;

