FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      boundaryT;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    body
    {
        type            fixedValue;
        value           uniform _Tb_;
    }
    free
    {
        type            fixedValue;
        value           uniform 1;
    }
    frontAndBack
    {
        type            empty;
    }
    symmetry
    {
        type            symmetryPlane;
    }
}

