FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     dsmcFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         _total_;

deltaT          0.01;

writeControl    runTime;

writeInterval   1;

purgeWrite      0;

writeFormat     ascii;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

functions
{
    fieldAverage1
    {
        type            fieldAverage;
        functionObjectLibs ( "libfieldFunctionObjects.so" );
        outputControl   outputTime;
        resetOnOutput   off;

        fields
        (
            rhoN
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            rhoM
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            dsmcRhoN
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            momentum
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            linearKE
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            internalE
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            iDof
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            q
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            fD
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            shearStress
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
            heatFlux
            {
                mean        on;
                prime2Mean  off;
                base        time;
                window      _w_;
            }
        );
    }
    dsmcFields1
    {
        type            dsmcFields;
        functionObjectLibs ( "libutilityFunctionObjects.so" );
        enabled         true;
        outputControl   outputTime;
    }
}

