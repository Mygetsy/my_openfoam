FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    back
    {
        type            empty;
    }
    front
    {
        type            empty;
    }
    outer
    {
        type            codedFixedValue;
        value           $internalField;
        redirectType    thermalCreepFixedValue;
    }
    inner
    {
        type            codedFixedValue;
        value           $internalField;
        redirectType    thermalCreepFixedValue;
    }
    bottom
    {
        type            symmetryPlane;
    }
    left
    {
        type            symmetryPlane;
    }
}

// ************************************************************************* //
