FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    D
    {
        solver          GAMG;
        tolerance       1e-11;
        relTol          0.9;
        smoother        GaussSeidel;
        nCellsInCoarsestLevel 20;
    }
    D_
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-11;
        relTol          0.9;
    }
}

stressAnalysis
{
    nCorrectors         1000;
    D                   2e-10;
    accumulateInterlayer    on;
}

