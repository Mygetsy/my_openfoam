FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         solidificationFoam;
startFrom           startTime;
startTime           0;
stopAt              endTime;
endTime             5e-3;
deltaT              .2e-5;
writeControl        runTime;
writeInterval       2e-5;
purgeWrite          0;
writeFormat         ascii;
writePrecision      8;
writeCompression    off;
timeFormat          fixed;
timePrecision       6;
adjustTimeStep      yes;
runTimeModifiable   yes;

