        equilibriumS    0.0914;
        equilibriumL    0.12;
        slopeS          -1226;
        slopeL          -862.2;
        diffusionS      6.8e-12; // 1.0e-14;
        diffusionL      2.0e-9;
    }

    Mo
    {
        molarMass       9.60e-2;
        equilibriumS    0.0288;
        equilibriumL    0.025;
        slopeS          6990;
        slopeL          7877;
        diffusionS      1.3e-11; // 1.0e-14;
        diffusionL      2.0e-9;
