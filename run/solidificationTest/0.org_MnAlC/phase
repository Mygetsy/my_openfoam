FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      phase;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    bottom
    {
        type fixedGradient;
        gradient uniform 1e-5;
    }
    top
    {
        type fixedGradient;
        gradient uniform -1e-5;
    }
    leftAndRight
    {
        type symmetryPlane;
    }
    frontAndBack
    {
        type empty;
    }
}

