FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (metal gas);

metal
{
    transportModel  Newtonian;
    nu              5.65e-07;   // Kim: nu = 8.56e-07, mu = 6.42e-3
    rho             2475;       // Kim: at T = 1700

    quasiIncompressible true;

    rhoSolid        2700;
    dRhoDT.solid    (0 0);
    dRhoDT.liquid   (0 0);
}

gas
{
    transportModel  Newtonian;

    nu              1.e-4;
    rho             0.4;
}

sigma
{
    type            temperatureDependent;
    sigma           polynomial
    (
        (0.87     0)
        (0.0   1)
    );
}

beta            beta            [ K^-1 ] 0.0;
mushyCoeff      mushyCoeff      [ kg/m^3/s ] 5e+3; // 1e+12 for PDAS=750nm

